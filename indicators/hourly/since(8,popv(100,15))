@prefix calli: <http://callimachusproject.org/rdf/2009/framework#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix screener: <https://probabilitytrading.net/screener/2014/schema.ttl#> .

<> a <../../classes/Indicator> , screener:Indicator ;
    rdfs:comment "The price above 15% of the previous 8 days price range weighted by volume (in increments of 1 hours). Volume distribution is estimated by distributing the interval volume among the price range with the prices between open and close weighted twice as heavily." ;
    rdfs:label "8-day hourly Price above 15% of Volume" ;
    screener:expression "SINCE(8,POPV(100,15))" ;
    calli:administrator </auth/groups/admin> , </auth/groups/super> ;
    calli:editor </auth/groups/power> , </auth/groups/staff> ;
    calli:reader </auth/groups/public> , </auth/groups/system> ;
    screener:hasInterval <../../intervals/m60> ;
    screener:hasUnit <../../units/price> .
